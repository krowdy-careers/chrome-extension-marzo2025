{
    "manifest_version": 3,
    "name": "Wong Category Extension",
    "version": "1.0",
    "description": "Chrome extension ofr get products of category in wong.",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "type": "module"
      }
    ],
    "action": {
    "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions":["activeTab","scripting","storage", "tabs"],
    "host_permissions": [
      "<all_urls>"
    ]
  }